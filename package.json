{
  "name": "regular-router",
  "version": "1.3.1",
  "description": "stateman based router for regular",
  "main": "dist/regular-router.common.js",
  "module": "dist/regular-router.es.js",
  "unpkg": "dist/regular-router.js",
  "scripts": {
    "dev": "bili --watch",
    "build": "bili",
    "precommit": "lint-staged",
    "prettier": "prettier \"src/**/*.js\" --single-quote --write",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fengzilong/regular-router.git"
  },
  "keywords": [
    "stateman",
    "regular",
    "router"
  ],
  "author": "fengzilong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fengzilong/regular-router/issues"
  },
  "homepage": "https://github.com/fengzilong/regular-router#readme",
  "devDependencies": {
    "bili": "^0.17.4",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "prettier": "^1.2.2",
    "stateman": "^0.2.0"
  }
}
