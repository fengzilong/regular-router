{
  "name": "regular-router",
  "version": "0.1.1",
  "description": "stateman based router for regular",
  "main": "dist/regular-router.js",
  "scripts": {
    "dev": "onchange \"src\" -i -v -- npm run build",
    "build": "bili --format umd --compress",
    "precommit": "lint-staged",
    "prettier": "prettier \"src/**/*.js\" --single-quote --write"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fengzilong/regular-router.git"
  },
  "keywords": [
    "stateman",
    "regular",
    "router"
  ],
  "author": "fengzilong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fengzilong/regular-router/issues"
  },
  "homepage": "https://github.com/fengzilong/regular-router#readme",
  "devDependencies": {
    "bili": "^0.14.2",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "onchange": "^3.2.1",
    "prettier": "^1.2.2",
    "stateman": "^0.2.0"
  }
}
